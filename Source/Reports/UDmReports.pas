function TdmReport.SmallIntArray(Vetor: array of Double): Double;
var
   I: Integer;
begin
 Result:= 0;
 for I:= 0 to Length(Vetor) - 1 do
     begin
      if Result = 0 then
         Result:= Vetor[I];
      if Vetor[I] > 0 then
         if Vetor[I] < Result then
            Result:= Vetor[I];
     end;
end;

